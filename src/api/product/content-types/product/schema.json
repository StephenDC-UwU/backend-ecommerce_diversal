{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "product_name"
    },
    "product_name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "decimal"
    },
    "origin": {
      "type": "enumeration",
      "enum": [
        "Africa",
        "Asia",
        "America",
        "Europe"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "gender": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gender.gender",
      "mappedBy": "product"
    },
    "sub_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sub-category.sub-category",
      "mappedBy": "product"
    },
    "colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::color.color",
      "mappedBy": "product"
    }
  }
}
